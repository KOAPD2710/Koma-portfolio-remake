{
	"name": "Koma-Portfolio",
	"version": "1.0.0",
	"description": "",
	"source": "./src/index.html",
	"staticFiles": {
		"staticOutPath": "./assets",
		"staticPath": "./assets"
	},
	"targets": {},
	"scripts": {
		"clear": "rimraf dist .parcel-cache",
		"dev": "yarn clear && cross-env PARCEL_WORKERS=1 parcel -p 3000",
		"build": "yarn clear && cross-env PARCEL_WORKERS=1 parcel build --no-cache --no-content-hash --public-url ./ --no-scope-hoist",
		"server": "nodemon server.js",
		"start": "concurrently \"yarn dev\" \"yarn server\""
	},
	"type": "commonjs",
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.1",
		"parcel": "^2.12.0",
		"parcel-reporter-static-files-copy": "^1.5.3",
		"rimraf": "^4.0.0",
		"sharp": "^0.31.1",
		"stripe": "^14.14.0"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead, Safari 14",
	"dependencies": {
		"@parcel/transformer-sass": "^2.12.0",
		"cors": "^2.8.5"
	}
}
